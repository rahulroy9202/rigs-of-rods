import * from "managed_mats.material"

material a474UID_invisible
{
   technique
   {
      pass
      {
         ambient 0.0 0.0 0.0 0.0
         diffuse 0.0 0.0 0.0 0.0
         scene_blend alpha_blend  
                        depth_write off
      }

   }

}

material invisible
{
   technique
   {
      pass
      {
         ambient 0.0 0.0 0.0 0.0
         diffuse 0.0 0.0 0.0 0.0
         scene_blend alpha_blend  
                        depth_write off
      }

   }

}


material a474UID_rails: RoR/Managed_Mats/Base
{
	set_texture_alias diffuse_tex train_rails.dds
}


